
{
    "builds": [
      {
        "src": "prisonsystem/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "prisonsystem/wsgi.py"
      },
      {
        "src": "coreapp/static/(.*)",
        "dest": "coreapp/static/$1"
      }
    ]
  }